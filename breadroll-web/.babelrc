{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime",
      ["prismjs", {
        "languages": ["javascript", "css", "markup","java","html","xml","go","git","php","bash","python","markdown"],
        "plugins": ["line-highlight","line-numbers","highlight-keywords","copy-to-clipboard"],
        "theme": "tomorrow",
        "css": true
      }]
    ]
}
